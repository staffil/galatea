{
  "name": "@apps-in-toss/native-modules",
  "type": "module",
  "version": "1.3.0",
  "description": "Native Modules for Apps In Toss",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./async-bridges": "./src/async-bridges.ts",
    "./constant-bridges": "./src/constant-bridges.ts",
    "./event-bridges": "./src/event-bridges.ts",
    "./bridges-meta.json": "./dist/bridges-meta.json",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@babel/runtime": "^7",
    "@granite-js/native": "0.1.28",
    "@granite-js/react-native": "0.1.28",
    "@types/react": "18.3.3",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "0.25.5",
    "picocolors": "^1.1.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "tsup": "^8.5.0",
    "typescript": "4.9.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@apps-in-toss/types": "^1.3.0",
    "es-toolkit": "^1.39.3"
  },
  "peerDependencies": {
    "@granite-js/native": "*",
    "@granite-js/react-native": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "71845fd4a349b630c91f07c3ab3d14f81774335a"
}
