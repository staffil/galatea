{
  "name": "@apps-in-toss/framework",
  "type": "module",
  "version": "1.3.0",
  "description": "The framework for Apps In Toss",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --no-watch",
    "test:no-parallel": "vitest --no-watch --config=vitest.no-parallel.config.mts",
    "lint": "eslint .",
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "import": "./dist/internal.js",
      "require": "./dist/internal.cjs"
    },
    "./bridge-entry": "./src/bridge-entry.ts",
    "./bridge-meta": {
      "types": "./dist/bridge-meta.d.ts",
      "default": "./dist/bridge-meta.js"
    },
    "./jest": {
      "types": "./dist/jest/index.d.ts",
      "import": "./dist/jest/index.js",
      "require": "./dist/jest/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "src/bridge-entry.ts",
    "src/async-bridges.ts",
    "src/constant-bridges.ts",
    "src/event-bridges.ts",
    "hermesc/**/*",
    "plugins.d.ts",
    "jest.d.ts"
  ],
  "bin": {
    "ait": "./bin/ait.js"
  },
  "dependencies": {
    "@apps-in-toss/analytics": "1.3.0",
    "@apps-in-toss/cli": "1.3.0",
    "@apps-in-toss/native-modules": "1.3.0",
    "@apps-in-toss/plugins": "1.3.0",
    "@apps-in-toss/types": "1.3.0",
    "es-hangul": "^2.3.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7",
    "@granite-js/mpack": "0.1.28",
    "@granite-js/native": "0.1.28",
    "@granite-js/plugin-core": "0.1.28",
    "@granite-js/react-native": "0.1.28",
    "@granite-js/utils": "0.1.28",
    "@toss/tds-react-native": "^1.3.3",
    "@types/kill-port": "^2.0.3",
    "@types/react": "18.3.3",
    "es-toolkit": "^1.34.1",
    "eslint": "^9.7.0",
    "execa": "^9.5.2",
    "jsdom": "^25.0.1",
    "kill-port": "^2.0.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "tsup": "^8.3.5",
    "typescript": "5.8.3",
    "vitest": "^3.0.3",
    "workspace-tools": "^0.38.2"
  },
  "peerDependencies": {
    "@granite-js/native": "*",
    "@granite-js/react-native": "*",
    "@toss/tds-react-native": ">=1.2.1",
    "@types/react": "*",
    "react": "*",
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "71845fd4a349b630c91f07c3ab3d14f81774335a"
}
